! ===================
!       urxvt
! ===================
! echo -e '\xEE\x82\xB0'
!*.font xft:Liberation Mono For Powerline:size=20

URxvt*scrollBar:	    false
!URxvt.letterSpace:   4
URxvt.internalBorder: 10
!URxvt*font: 		     xft:Inconsolata Nerd Font Mono:pixelsize=27:minspace=False, xft:DejaVu Sans:pixelsize=18
!URxvt*boldFont:		     xft:Inconsolata Nerd Font Mono:pixelsize=27:minspace=False, xft:DejaVu Sans:pixelsize=18
!URxvt*cursorBlink:    true
!URxvt.iso14755:		    false
!URxvt*utf8:			      true
URxvt.keysym.C-equal:      resize-font:bigger
URxvt.keysym.C-parenright: resize-font:smaller
!URxvt.letterSpace: -2


URxvt*font:                 xft:Inconsolata Nerd Font Mono:pixelsize=17:minspace=False, xft:DejaVu Sans:pixelsize=10
URxvt*boldFont:             xft:Inconsolata Nerd Font Mono:pixelsize=17:minspace=False, xft:DejaVu Sans:pixelsize=10
URxvt*letterSpace: -1
!URxvt*font: xft:Inconsolata Nerd Font Mono:size=15, xft:DejaVu Sans:size=12



! regular
!Rxvt*.font: \
!        xft:Source Code Pro:size=12, \
!        xft:Noto Mono:size=12, \
!        xft:DejaVu Sans Mono:size=12, \
!        xft:Unifont:style=medium:size=12
! bold
!Rxvt*.boldfont: \
!        xft:Source Code Pro:size=12:style=bold, \
!        xft:Noto Mono:size=12:style=bold, \
!        xft:DejaVu Sans Mono:size=12:style=bold, \
!        xft:Unifont:size=12:style=bold
! italic    
!Rxvt*.italicfont: \
!        xft:Source Code Pro:size=12:style=italic, \
!        xft:Noto Mono:size=12:style=italic, \
!        xft:DejaVu Sans Mono:size=12:style=italic, \
!        xft:Unifont:size=12:style=italic
! bold+italic left as an exercise for the reader.

!Keybinding
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard


! =================
!       Xft
! =================
Xft.dpi:          	96
Xft.antialias:     	true
Xft.rgba:			      subpixel
Xft.hinting:       	true
Xft.hintstyle:		  hintslight
!Xft.lcdfilter:	  lcddefault

